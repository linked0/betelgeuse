{
  "name": "pooh-land-backend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/poohgithub/pooh-land-backend.git",
  "author": "linked0 <linked0@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "./run_local.sh",
    "start-no-db": "./run_local_no_db.sh",
    "start:refresh": "./run_local_refresh.sh",
    "dev": "ts-node-dev ./src/index.ts",
    "build": "tsc",
    "clean": "docker compose --env-file .env down && rm -rf dist && sleep 2 && rm -rf data",
    "dev-db": "docker compose --env-file .env up -d db",
    "dev-db-down": "docker compose --env-file .env down",
    "clean:dev": "docker compose --env-file .env.dev down && rm -rf dist && sleep 2 && rm -rf data",
    "schema:dev": "docker compose --env-file .env.dev up -d db",
    "migration-up": " npx mikro-orm  migration:up",
    "migration-create": " npx mikro-orm  migration:create",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "drop-schema": "ts-node drop-schema"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.245.0",
    "@mikro-orm/cli": "^5.6.6",
    "@mikro-orm/core": "^5.6.6",
    "@mikro-orm/migrations": "^5.6.6",
    "@mikro-orm/postgresql": "^5.6.6",
    "@mikro-orm/seeder": "5.6.6",
    "@mikro-orm/sql-highlighter": "^1.0.1",
    "@types/got": "^9.6.12",
    "@types/morgan": "^1.9.4",
    "apollo-server-express": "^3.11.1",
    "body-parser": "^1.20.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ethers": "^5.7.2",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-promise-router": "^4.1.1",
    "generate-api-key": "^1.0.2",
    "got": "^11.8.2",
    "graphql": "^15.3.0",
    "graphql-upload-ts": "^2.0.5",
    "graphql-ws": "^5.11.2",
    "helmet": "^6.0.1",
    "image-downloader": "^4.3.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "sharp": "^0.31.3",
    "shortid": "^2.2.16",
    "swagger-autogen": "^2.23.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.1",
    "ts-node": "^10.9.2",
    "type-graphql": "^1.2.0-rc.1",
    "typescript": "^4.9.4",
    "uuid": "^9.0.0",
    "ws": "^8.12.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/sharp": "^0.31.1",
    "@types/uuid": "^9.0.0",
    "@types/ws": "^8.5.4",
    "prettier": "^2.5.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.0.0",
    "tslint": "6.1.3"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
